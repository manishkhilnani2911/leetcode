class Solution {
public:
    int maxDepth(TreeNode* root) {
      if (root == NULL) {
        return 0;
      } else if (!root->left && !root->right) {
        return 1;
      }   
      if (!root->left) {
        return maxDepth(root->right) + 1;
      }
      if (!root->right) {
        return maxDepth(root->left) + 1;
      }
      return max(maxDepth(root->left),maxDepth(root->right)) + 1;
    }
};
